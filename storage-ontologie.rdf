		<usdl:hasTechnicalInterface rdf:resource="http://www.w3.org/TR/soap/" />
	</usdl:InteractionProtocol>
	<usdl:InteractionProtocol rdf:about="http://linked-usdl.org/ns/usdl/storage-as-a-service#REST">
